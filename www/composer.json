{
  "name": "author/myproject",
  "type": "silverstripe-project",
  "require": {
      "php": ">=7.0.0",
      "silverstripe/assets": "^1.2@stable",
      "silverstripe/config": "^1@stable",
      "silverstripe/framework": "^4.2@stable",
      "silverstripe/admin": "^1.2@stable",
      "silverstripe/asset-admin": "^1.2@stable",
      "silverstripe/campaign-admin": "^1.2@stable",
      "silverstripe/cms": "^4.2@stable",
      "silverstripe/errorpage": "^1.2@stable",
      "silverstripe/graphql": "^2@stable",
      "silverstripe/reports": "^4.2@stable",
      "silverstripe/siteconfig": "^4.2@stable",
      "silverstripe/versioned": "^1.2@stable"
  },
  "require-dev": {
      "phpunit/phpunit": "^5.7"
  },
  "extra": {
      "project-files-installed": [
          "app/.htaccess",
          "app/_config.php",
          "app/_config/mysite.yml",
          "app/src/Page.php",
          "app/src/PageController.php"
      ],
      "public-files-installed": [
          "silverstripe.php",
          "index.php"
      ],
      "expose": [
        "themes/mytheme/css",
        "themes/mytheme/javascript",
        "themes/mytheme/images"
      ]
  },
  "config": {
      "process-timeout": 600
  },
  "prefer-stable": true,
  "minimum-stability": "dev"
}